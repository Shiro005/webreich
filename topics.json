            "sections": [
              {
                "title": "Selectors",
                "content": "CSS selectors target HTML elements for styling.",
                "codeExample": ".class-name {\n  color: orange;\n}\n\n#id-name {\n  background: black;\n}",
                "terminalCommands": []
              },
              {
                "title": "Box Model",
                "content": "The CSS box model is the foundation of layout on the Web.",
                "codeExample": ".box {\n  margin: 10px;\n  padding: 20px;\n  border: 1px solid orange;\n}",
                "terminalCommands": []
              },
              {
                "title": "Flexbox",
                "content": "Flexbox is a layout model for arranging elements in rows or columns.",
                "codeExample": ".container {\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n}",
                "terminalCommands": []
              }
